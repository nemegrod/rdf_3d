#############################
# Locations & Habitat Areas
#############################

ex:USA a :Country ; rdfs:label "United States of America" .
ex:Mexico a :Country ; rdfs:label "Mexico" .
ex:Brazil a :Country ; rdfs:label "Brazil" .
ex:Colombia a :Country ; rdfs:label "Colombia" .
ex:Argentina a :Country ; rdfs:label "Argentina" .
ex:Belize a :Country ; rdfs:label "Belize" .

ex:Arizona a :State ; :locatedInCountry ex:USA ; rdfs:label "Arizona" .
ex:Sonora a :State ; :locatedInCountry ex:Mexico ; rdfs:label "Sonora" .

ex:WhetstoneMountains a :MountainRange ; rdfs:label "Whetstone Mountains" .
ex:SantaRitaMountains a :MountainRange ; rdfs:label "Santa Rita Mountains" .
