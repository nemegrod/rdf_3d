# Habitat areas used in corridors/reserves/wetlands
ex:NorthernJaguarReserveArea a :HabitatArea ;
  rdfs:label "Northern Jaguar Reserve (Sonora)" ;
  :hasAcreage 60300 .

ex:SkyIslandsBorderlands a :HabitatArea ;
  rdfs:label "Arizona–Sonora Sky Islands borderlands" .

ex:IberaWetlandsArea a :HabitatArea ;
  rdfs:label "Iberá Wetlands (Argentina)" .

#############################
# Habitats (and where they are)
#############################

ex:PantanalWetland a :Wetland ; :locatedIn ex:PantanalRegion ; rdfs:label "Pantanal wetland habitat" .
