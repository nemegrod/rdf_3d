@prefix : <http://example.org/ontology#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

#############################
# Original Ontology Classes #
#############################

:Animal a owl:Class .

:Mammal a rdfs:Class ;
    rdfs:subClassOf :Animal .

:BigCat a rdfs:Class ;
    rdfs:subClassOf :Mammal .

:Jaguar a rdfs:Class ;
    rdfs:subClassOf :BigCat ;
    :diet :Carnivore ;
    :lifespan 12 ;
    :scientificName "Panthera onca" .

:Habitat a rdfs:Class .

:DietType a rdfs:Class .


:Country a rdfs:Class ;
    rdfs:label "Country" ;
    rdfs:comment "A sovereign state or nation." .

:State a rdfs:Class ;
    rdfs:label "State" ;
    rdfs:comment "A sub-national administrative division, such as a state or province." .


#############################
# Ontology Properties #
#############################


:locatedInCountry a rdf:Property ;
    rdfs:domain :State ;
    rdfs:range :Country ;
    rdfs:comment "Specifies the country in which a state is located." .

:locatedInState a rdf:Property ;
    rdfs:domain :Habitat ;
    rdfs:range :State ;
    rdfs:comment "Specifies the state or administrative region in which a habitat is located." .

:occursInState a rdf:Property ;
    rdfs:domain :Animal ;
    rdfs:range :State ;
    rdfs:comment "Indicates a state where an animal has been observed or is known to occur." .

:name a rdf:Property ;
    rdfs:domain :Animal ;
    rdfs:range xsd:string .

:habitat a rdf:Property ;
    rdfs:domain :Animal ;
    rdfs:range :Habitat .

:diet a rdf:Property ;
    rdfs:domain :Animal ;
    rdfs:range :DietType .

:lifespan a rdf:Property ;
    rdfs:domain :Animal ;
    rdfs:range xsd:integer .

:scientificName a rdf:Property ;
    rdfs:domain :Animal ;
    rdfs:range xsd:string .



